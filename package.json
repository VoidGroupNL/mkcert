{
    "name": "@voidgroup/mkcert",
    "version": "0.0.1-dev.6",
    "description": "A Package To Generate CA/DNS SSL Certificates",
    "keywords": [
        "openssl",
        "generate",
        "ssl",
        "certificate",
        "authority"
    ],
    "homepage": "https://github.com/VoidGroupNL/mkcert",
    "bugs": {
        "url": "https://github.com/VoidGroupNL/mkcert/"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "author": {
        "name": "VoidStoneDev",
        "url": "https://www.npmjs.com/~voidstonedev"
    },
    "main": "out/app.js",
    "bin": {
        "mkcert": "out/app.js"
    },
    "man": [
        "man/mkcert.1"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/VoidGroupNL/mkcert.git"
    },
    "scripts": {
        "build": "rm -r -d -f ./out/*** && npx tsc && npm run copy-data",
        "copy-data": "rsync --exclude=\"***.ts\" --exclude=\"***.js\" -r ./src/*** ./out",
        "prepare": "npm run build"
    },
    "engines": {
        "node": "^15.5.0",
        "npm": "^7.3.0"
    },
    "os": [
        "linux",
        "win32",
        "linux,win32"
    ],
    "cpu": [
        "x64",
        "ia86",
        "arm",
        "mips"
    ],
    "dependencies": {
        "@types/node": "^14.14.20",
        "typescript": "^4.1.3"
    }
}
